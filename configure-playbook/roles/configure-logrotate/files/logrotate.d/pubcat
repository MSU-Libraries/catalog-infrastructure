/var/lib/docker/volumes/*_logs/_data/*/*.log /var/lib/docker/volumes/traefik_logs/_data/*.log {
  rotate 7
  daily
  compress
  missingok
  delaycompress
  copytruncate
  prerotate
    bash -c "[[ ! $1 =~ _latest ]]"
  endscript
  postrotate
    docker ps | grep traefik | awk '{print $1}' | xargs -I {} sh -c "echo docker kill signal=USR1 {}"
  endscript
}
